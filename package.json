{
  "name": "egg-example-hackernews",
  "version": "1.0.0",
  "description": "hackernews showcase for egg",
  "private": true,
  "dependencies": {
    "egg": "^1.4.0",
    "egg-scripts": "^2.1.0",
    "egg-static": "^1.4.1",
    "egg-view-nunjucks": "^2.1.2",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "autod-egg": "^1.0.0",
    "cheerio": "^1.0.0-rc.1",
    "egg-bin": "^3.4.1",
    "egg-mock": "^3.7.1",
    "eslint": "^6.5.1",
    "eslint-config-egg": "^4.2.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "prd-start": "egg-scripts start --daemon --port=8080",
    "prd-stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  }
}
